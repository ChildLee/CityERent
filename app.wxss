page {
  font-size: 32rpx; }

.border {
  position: relative; }
  .border:after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 200%;
    height: 200%;
    border-bottom: 1px solid #e0e0e0;
    box-sizing: border-box;
    transform: scale(0.5);
    transform-origin: 0 0;
    pointer-events: none; }

.arrow {
  content: " ";
  display: inline-block;
  position: absolute;
  top: 50%;
  right: 0;
  border-width: 1px 1px 0 0;
  border-style: solid;
  border-color: #C0C0C0;
  width: 6px;
  height: 6px;
  margin-top: -4px;
  transform: rotateZ(45deg); }

.small {
  font-size: .8em; }

.bd {
  border: 1px solid red; }

